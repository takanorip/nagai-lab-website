---
import Head from '../../components/common/Head.astro';
import Header from '../../components/common/Header';
import Footer from '../../components/common/Footer';
import Research from '../../components/research/Research';
const data = await Astro.fetchContent('../../pages/research/*.md');
const topicsData = data.sort((a, b) => b.year - a.year)
---
<html lang="ja">
<head>
  <Head title="RESEARCH | 大阪大学基礎工学研究科 長井研究室" />
</head>
<body>
  <Header client:load location="research" />
  <Research topics={topicsData} client:load />
  <Footer />
</body>
</html>